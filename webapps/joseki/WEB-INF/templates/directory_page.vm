<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en" class="browser">
  <head>
    <title>
      All $classmap | D2R Server
    </title>
    <link rel="stylesheet" type="text/css" href="${home_link}snorql/style.css" />
    <link rel="alternate" type="application/rdf+xml" href="$rdf_link" title="RDF" />
  </head>
  <body>
    <div id="rdficon"><a href="$rdf_link" title="RDF data"><img src="/snorql/rdf_flyer.24.gif" alt="[RDF data]" /></a></div>
    <div id="header">
      <h1 id="title">
        All $classmap
      </h1>
      <div class="subtitle">&nbsp;</div>
    </div>

    <div class="section">
      <strong><a href="$home_link">Home</a></strong>
#if (!$classmap_links.isEmpty())
      | 
#foreach ($aClassMap in $classmap_links.keySet())
#if ($aClassMap == $classmap)
      <strong>$aClassMap</strong>
#else
      <a href="$classmap_links.get($aClassMap)">$aClassMap</a>
#end
#end
#end
    </div>
    <div class="section">
      <ul class="directory">
#foreach ($resource as $resources.keySet())
        <li>
#if ($resource == $resources.get($resource))
          <a href="$resource">$resource</a>
#else
          <a href="$resource">$resources.get($resource)</a><br />
          <small>$resource</small>
#end
        </li>
#end
      </ul>
    </div>

    <div id="footer">Generated by <a href="http://www.wiwiss.fu-berlin.de/suhl/bizer/d2r-server/">D2R Server</a></div>
  </body>
</html>
